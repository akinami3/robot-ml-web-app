    "@types/nipplejs": "^0.10.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.5.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prettier": "^3.1.1"
